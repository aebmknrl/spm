{
  "name": "udemy-webserver",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./",
    "migrate": "sequelize db:migrate",
    "undomigrate": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "unseed": "sequelize db:seed:undo",
    "refreshdb": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "colors": "^1.3.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "mysql2": "^1.6.4",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  }
}