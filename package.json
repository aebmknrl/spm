    "jsonwebtoken": "^8.4.0",
    "moment": "^2.23.0",
    "mysql2": "^1.6.4",
    "nodemailer": "^6.2.1",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
